{
  "manifest_version" : 3,
  "name": "YTP Tags",
  "version": "0.1",
  "description": "Tags for YouTube Playlists",
  "author": "Sathwik C",
  "permissions" : ["storage", "tabs"],
  "content_scripts" : [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action" : {
    "default_popup" : "popup.html",
    "default_title" : "YTP Tags"
  }
}